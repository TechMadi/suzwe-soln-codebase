{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error" , {
            "endOfLine": "auto"
          }],
          "no-unused-vars": "warn",
          "react/jsx-props-no-spreading": "off",
          "no-param-reassign" : "off",
          "import/no-named-as-default": 0,
          "react/prop-types": "off"
    }
}
